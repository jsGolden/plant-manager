{
  "expo": {
    "name": "PlantManager",
    "slug": "PlantManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rocketseat.plantmanager",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.rocketseat.plantmanager",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Aplicação criada para lembrar o usuário de regar suas plantinhas pelo bem do meio ambiente!",
    "githubUrl": "https://github.com/jsGolden/plant-manager"
  }
}
